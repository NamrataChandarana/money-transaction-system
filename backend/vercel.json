{
    "version": 2,
    "name": "paytm-wallet-app-backend",
    "builds": [{ "src": "index.js", "use": "@vercel/node" }],
    "routes": [{ 
        "src": "/(.*)", 
        "dest": "/index.js",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
        "headers": {
            "Access-Control-Allow-Origin": "http://localhost:5173/"
        } }]
}